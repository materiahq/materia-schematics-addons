{
    "name": "<%= utils.dasherize(packageName) %>",
    "version": "0.1.0",
    "description": "Starter kit to create a Materia Addon",
    "author": "team@getmateria.com",
    "license": "MIT",
    "scripts": {
      "build": "ng build",
      "e2e": "ng e2e",
      "lint": "ng lint",
      "ng": "ng",
      "start": "ng serve",
      "test": "ng test",
      "build:prod": "npm run clean && npm run build:client && npm run build:server && npm run postbuild",
      "build:server": "tsc -p ./tsconfig.server.json && ncp ./server/api.json ./dist/server/api.json",
      "build:client": "ng build --prod",
      "postbuild": "node scripts/postbuild.js",
      "clean": "rimraf dist"
    },
    "peerDependencies": {
      "@angular/common": "~7.2.0",
      "@angular/core": "~7.2.0",
      "@angular/material": "~7.3.7",
      "@angular/cdk": "~7.3.7"
    },
    "devDependencies": {
      "@angular-devkit/build-angular": "~0.13.0",
      "@angular-devkit/build-ng-packagr": "~0.13.0",
      "@angular/animations": "~7.2.0",
      "@angular/cdk": "~7.3.7",
      "@angular/cli": "~7.3.8",
      "@angular/common": "~7.2.0",
      "@angular/compiler": "~7.2.0",
      "@angular/compiler-cli": "~7.2.0",
      "@angular/core": "~7.2.0",
      "@angular/flex-layout": "7.0.0-beta.24",
      "@angular/forms": "~7.2.0",
      "@angular/http": "~7.2.0",
      "@angular/language-service": "~7.2.0",
      "@angular/material": "~7.3.7",
      "@angular/platform-browser": "~7.2.0",
      "@angular/platform-browser-dynamic": "~7.2.0",
      "@materia/addons": "1.0.0-beta.2",
      "@materia/interfaces": "1.0.0-beta.9",
      "@types/jasmine": "2.8.8",
      "@types/jasminewd2": "2.0.3",
      "@types/node": "~8.9.4",
      "codelyzer": "~4.5.0",
      "core-js": "^2.5.4",
      "jasmine-core": "~2.99.1",
      "jasmine-spec-reporter": "~4.2.1",
      "karma": "~4.0.0",
      "karma-chrome-launcher": "~2.2.0",
      "karma-coverage-istanbul-reporter": "~2.0.1",
      "karma-jasmine": "~1.1.2",
      "karma-jasmine-html-reporter": "^0.2.2",
      "ncp": "^2.0.0",
      "ng-packagr": "^4.2.0",
      "protractor": "~5.4.0",
      "rxjs": "6.3.3",
